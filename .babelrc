{
    "presets": [
        ["@babel/preset-env", {
        "targets": {
          "node": "current"
        }}]
    ],
    "plugins": [
      ["module-resolver", {
        "root": [
            "./src"
        ],
        "alias": {
          "filepaths": "./filepaths.js"
        }
      }],
     "@babel/plugin-proposal-class-properties",
    ]
  }